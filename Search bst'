#define MAX 1000
int tree[MAX];
int root =0;
void initialize(){
    for (int i=0;i<MAX;i++){
        tree[i]=-1;
    }
}
 void insert(int root,int value){
     if(tree[root]== -1){
         tree[root]=value;
         }
     else if(value<tree[root]){
         insert(2*root+1,value);
     }
     else if(value> tree[root])
     {
         insert(2*root+2,value);
     } 
 }
